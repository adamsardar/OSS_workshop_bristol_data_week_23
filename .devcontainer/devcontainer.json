{
    "name": "Vanilla Ubuntu R4.3",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
        "ghcr.io/rocker-org/devcontainer-features/quarto-cli:latest": {
            "version": "1",
            "installChromium": true
        },
        "ghcr.io/rocker-org/devcontainer-features/apt-packages:latest": {
            "packages": "chromium,libudunits2-dev,libxtst6,libxt6,libmagick++-dev"
        },
        "ghcr.io/rocker-org/devcontainer-features/r-packages:latest": {
            "packages": "tidyverse,here,httpgd,dplyr"
        },
        "ghcr.io/rocker-org/devcontainer-features/r-rig:latest": {
            "version": "4.3",
            "installDevTools": true,
            "installRMarkdown": true,
            "installRadian": true
        }
    },
    "remoteEnv": {
        "PKG_SYSREQS": "true"
    },
    "customizations": {
        "codespaces": {
            "openFiles": ["README.md"]
        }
    }
}
